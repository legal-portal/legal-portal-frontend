{
  "name": "demo-worker",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "npm run format:prettier && npm run format:es",
    "format:prettier": "prettier --write .",
    "format:es": "eslint \"./src/**/*.ts\" --fix",
    "lint": "npm run lint:prettier && npm run lint:typescript && npm run lint:es",
    "lint:typescript": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "lint:es": "eslint \"./src/**/*.ts\"",
    "precommit:format": "prettier --write --ignore-unknown",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:build": "npx nest build",
    "db:migrate": "echo no-op",
    "start:app": "node dist/main.js"
  },
   "files": [
    "dist",
    "entrypoint.sh"
  ],
  "dependencies": {
    "bull": "^4.16.5",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "latest",
    "@nestjs/platform-express": "latest",
    "reflect-metadata": "^0.2.2",
    "rxjs": "latest"
  },
  "devDependencies": {
    "@tooling/jest": "*",
    "@nestjs/cli": "latest",
    "typescript": "latest",
    "@tooling/prettier": "*",
    "@tooling/nestjs-webpack": "*",
    "@tooling/eslint-config-custom": "*",
    "@tooling/typescript": "*",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
