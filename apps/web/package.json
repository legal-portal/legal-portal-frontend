{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3200",
    "build": "npx next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "node .next/standalone/apps/admin/server.js",
    "test": "jest",
    "format": "npm run format:prettier && npm run format:es",
    "format:prettier": "prettier --write .",
    "format:es": "eslint . --fix",
    "lint": "npm run lint:prettier && npm run lint:typescript && npm run lint:es",
    "lint:typescript": "tsc --noEmit",
    "lint:prettier": "prettier --check .",
    "lint:es": "eslint .",
    "precommit:format": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.515.0",
    "next": "15.3.3",
    "next-intl": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tooling/eslint-config-custom": "*",
    "@tooling/jest": "*",
    "@tooling/prettier": "*",
    "@tooling/typescript": "*",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
